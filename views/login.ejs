  <form class="form-user">
    <h2>Ingresá con tu correo</h2>

    <div class="form-group">
      <label for="InputEmail1">Email:</label>
      <input
        type="email"
        class="form-control margin-input"
        id="InputEmail1"
        aria-describedby="emailHelp"
        required
      />
    </div>
    <div class="form-group">
      <label for="InputPassword1">Contraseña:</label>
      <input
        type="password"
        class="form-control margin-input"
        id="InputPassword1"
        required
      />
    </div>

    <a href="./bottomMobileNavbar.html" type="submit" class="button">Enviar</a>
  </form>

